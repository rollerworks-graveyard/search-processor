{
    "name": "rollerworks/search-processor",
    "description": "SearchProcess request handling for RollerworksSearch",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": "^7.1",
        "rollerworks/search": "^2.0@dev",
        "rollerworks/uri-encoder": "^1.1.0 || ^2.0",
        "psr/http-message": "^1.0.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4.2 || ^4.0.2",
        "symfony/psr-http-message-bridge": "^1.0.0",
        "zendframework/zend-diactoros": "^1.4.0",
        "psr/simple-cache": "^1.0.0",
        "phpunit/phpunit": "^6.5.4"
    },
    "suggest": {
        "psr/simple-cache": "To use the CachedSearchProcessor"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Processor\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Processor\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
